doctype html
html
    head
        meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no')
        link(rel='alternate', type='application/rss+xml', title='Your Feed Title', href='https://www.cipher-com.herokuapp.com/rss')
        link(href='https://fonts.googleapis.com/css?family=Oswald', rel='stylesheet', type='text/css')
        link(href='https://fonts.googleapis.com/css?family=Open+Sans', rel='stylesheet', type='text/css')
        link(href='https://fonts.googleapis.com/css?family=Roboto', rel='stylesheet', type='text/css')
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css')
        link(rel='stylesheet', href='/res/css/style.css', media='screen', title='no title')
        script(src='https://code.jquery.com/jquery-1.11.3.min.js', type='text/javascript')
        script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js', type='text/javascript')
        script(type='text/javascript', src='/res/js/main.js')
        block head
        title Cipher Com
    body
        nav.navbar.navbar-inverse
            .container-fluid
                .navbar-header
                    button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand.logo-a(href='/')
                        img(src='/res/img/brand_logo_new.png', alt='Cipher Logo', width='40', height='45')
                #bs-example-navbar-collapse-1.collapse.navbar-collapse
                    ul.nav.navbar-nav
                        li
                            a#blog-link(href='/blog') Blog
                        li
                            a#archive-link(href='/archive') Archive
                    ul.nav.navbar-nav.navbar-right
                        li
                            a#rss-link(href='/rss') RSS Feed
        script.
          ((window.gitter = {}).chat = {}).options = {
            room: 'kinpat7/WebDev-Group-Project'
          };
        script(src='https://sidecar.gitter.im/dist/sidecar.v1.js', async='', defer='')
        block content
        .footer
            p Cipher-com 2015
